{
  "name": "reinainfo",
  "repository": "git@github.com:yarnaimo/reinainfo.git",
  "author": "yamaimo <yarnaimo@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.5.3"
  },
  "env": {
    "TZ": "Asia/Tokyo"
  },
  "scripts": {
    "ts-node": "ts-node",
    "test": "NODE_ENV=test jest",
    "testi": "NODE_ENV=test tsc && node --inspect-brk node_modules/.bin/jest --coverage false",
    "dev": "ts-node src/server",
    "start": "NODE_ENV=production ts-node -T src/server"
  },
  "dependencies": {
    "@slack/client": "^4.3.1",
    "axios": "^0.18.0",
    "class-validator": "^0.9.1",
    "config": "^1.31.0",
    "date-fns": "^2.0.0-alpha.11",
    "firebase-admin": "^5.13.1",
    "firestore-simple": "^1.0.1",
    "getopts": "^2.1.2",
    "js-yaml": "^3.12.0",
    "lazy.js": "^0.5.1",
    "micro": "^9.3.2",
    "microrouter": "^3.1.3",
    "oauth-authorization-header": "^0.0.7",
    "qs": "^6.5.2",
    "shell-quote": "^1.6.1",
    "ts-node": "^7.0.0",
    "ts-node-dev": "^1.0.0-pre.26",
    "ts-twitter": "^0.1.7",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/config": "^0.0.34",
    "@types/getopts": "^2.0.0",
    "@types/jest": "^23.3.0",
    "@types/js-yaml": "^3.11.2",
    "@types/lazy.js": "^0.3.29",
    "@types/micro": "^7.3.1",
    "@types/microrouter": "^3.1.0",
    "@types/node": "^10.5.3",
    "@types/qs": "^6.5.1",
    "@types/shell-quote": "^1.6.0",
    "jest": "^23.4.1",
    "ts-jest": "^23.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
