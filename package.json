{
  "name": "reinainfo",
  "private": true,
  "repository": "git@github.com:yarnaimo/reinainfo.git",
  "license": "MIT",
  "author": "yamaimo <yarnaimo@gmail.com>",
  "scripts": {
    "build": "tsc",
    "config-env": "ts-node config-env",
    "dev": "ts-node src/server",
    "start": "NODE_ENV=production node dist/server",
    "test": "NODE_ENV=test jest",
    "ts-node": "ts-node"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "@slack/client": "^4.3.1",
    "@tensorflow/tfjs": "^0.12.4",
    "@tensorflow/tfjs-node": "^0.1.12",
    "@yarnaimo/arraymo": "^1.0.1",
    "@yarnaimo/twimo": "^1.1.1",
    "class-validator": "^0.9.1",
    "date-fns": "^2.0.0-alpha.11",
    "dotenv": "^6.0.0",
    "firebase-admin": "^6.0.0",
    "firestore-simple": "^1.0.1",
    "getopts": "^2.1.2",
    "lazy.js": "^0.5.1",
    "micro": "^9.3.2",
    "microrouter": "^3.1.3",
    "qs": "^6.5.2",
    "shell-quote": "^1.6.1"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/getopts": "^2.0.0",
    "@types/jest": "^23.3.0",
    "@types/lazy.js": "^0.3.29",
    "@types/micro": "^7.3.1",
    "@types/microrouter": "^3.1.0",
    "@types/node": "^10.7.0",
    "@types/qs": "^6.5.1",
    "@types/shell-quote": "^1.6.0",
    "jest": "^23.4.1",
    "ts-jest": "^23.0.1",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.26",
    "typescript": "^3.0.3"
  }
}
